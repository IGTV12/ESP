local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("DarkHub", "GrapeTheme")
    -- MAIN
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("ESP")

    --non toggle buttons
    MainSection:NewButton("ESP", "you can see players", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/IGTV12/ESP/main/ESP'))()
    end) --make sure its in a raw pastebin url where PUT YOUR SCRIPT IS not THE SCRIPT ITSELF
    
	local MainSection = Main:NewSection("Silent Aim")
    MainSection:NewButton("Silent Aim", "Auto targets people", function(state)
		loadstring(game:HttpGet('https://raw.githubusercontent.com/IGTV12/ESP/main/Silent%20aim'))()
    end)
    -- how to make new tabs
    --RENAME THIS TO YOUR TAB NAME MAKE SURE IT HAS -- IN FRONT OF IT  
	local MainSection = Main:NewSection("Made By Blooper#6162")
